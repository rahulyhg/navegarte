{
  "name": "navegarte/framework",
  "description": "Aplicação para desenvolvimento de pequenos e grandes projetos.",
  "type": "project",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/vagnercardosoweb/navegarte/issues",
    "source": "https://github.com/vagnercardosoweb/navegarte"
  },
  "authors": [
    {
      "name": "Vagner Cardoso",
      "email": "vagner@vcwebnetworks.com.br",
      "homepage": "https://www.vcwebnetworks.com.br/",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.6",
    "navegarte/core": "2.*",
    "robmorgan/phinx": "^0.10.6"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/helpers.php"
    ]
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
