{
  "name": "navegarte/framework",
  "description": "Projeto para desenvolvimento do front end da navegarte!",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Vagner Cardoso",
      "email": "vagnercardosoweb@gmail.com",
      "homepage": "https://www.navegarte.com.br/",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.6.4",
    "navegarte/core": "*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"file_exists('.env') || copy('.env-example', '.env');\""
    ]
  },
  "config": {
    "optimize-autoload": true
  }
}
